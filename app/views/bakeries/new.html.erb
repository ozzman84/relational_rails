<%= form_with url: '/bakeries', method: :post, local: true do |form| %>
<%= form.label :name %>
<%= form.text_field :name %>
<%= form.label :city %>
<%= form.text_field :city %>
<%= form.label :delivery %>
<%= form.text_field :delivery %>
<%= form.label :review %>
<%= form.text_field :review %>
<%=form.submit "Create Bakery" %>
<% end %>
