<h3><%= "#{@guest.first_name} #{@guest.last_name}" %></h3>

  <ul>
    <li><%= "ID: #{@guest.id}" %></li>
    <li><%= "Rent: #{@guest.rent}" %></li>
    <li><%= "Visiting: #{@guest.visiting}" %></li>
    <li><%= "Created at: #{@guest.created_at}" %></li>
    <li><%= "Updated at: #{@guest.updated_at}" %></li>
  </ul>

<%= button_to 'Delete', "/guests/#{@guest.id}", method: :delete %>
<%= button_to "Edit", "/guests/#{@guest.id}/edit", method: :get %>
