<h1>All Beds</h1>

<% @beds.each do |bed| %>
  <h3><a href="/beds/<%= bed.id %>"><%= bed.name %></a></h3>
  <ul>
    <li>Occupied: <%= bed.occupied %></li>
    <li>Guess Count: <%= bed.guest_count %></li>
    <li>Created at: <%= bed.created_at %></li>
    <li>Updated at: <%= bed.updated_at %></li>
  </ul>
<% end %>

<h6><a href="/beds/new">New Brewery</a></h6>
